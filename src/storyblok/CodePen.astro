---
import { storyblokEditable } from "@storyblok/astro";

const { blok } = Astro.props;
---

<div
    class="mb-7 rounded-lg overflow-clip"
    {...storyblokEditable(blok)}
>
    <div set:html={blok.code} />
    <script
        async
        src="https://public.codepenassets.com/embed/index.js"
    >
    </script>
</div>
